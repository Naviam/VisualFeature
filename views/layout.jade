doctype 5
html(lang="en", ng-app)
    head
        title Visual Feature
        meta(charset="utf-8")
        meta(name="description",content="visual feature")
        meta(name="author",content="Vitali Hatalski")
        meta(name="viewport",content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet",href="/css/bootstrap.min.css")
        link(rel="stylesheet",href="/css/bootstrap-theme.min.css")
    body
        div.navbar.navbar-default.navbar-static-top
            div.container
                div.navbar-header
                    button.navbar-toggle(type="button",data-toggle="collapse",data-target=".navbar-collapse")
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand(href="#") Visual Feature
                div.navbar-collapse.collapse
                    ul.nav.navbar-nav
                        li.active
                            a(href="#") Home
                        li
                            a(href="#about") About
                        li
                            a(href="#contact") Contact
                        li.dropdown
                            a.dropdown-toggle(href="#",data-toggle="dropdown") Dropdown
                                b.caret
                            ul.dropdown-menu
                                li
                                    a(href="#") Action
                                li
                                    a(href="#") Another Action
                                li
                                    a(href="#") Something else here
                                li.divider
                                li.dropdown-header Nav header
                                li
                                    a(href="#") Separated link
                                    a(href="#") One more separated link
                    ul.nav.navbar-nav.navbar-right
                        li
                            a(href="../navbar/") Default
                        li.active
                            a(href="./") Static top
                        li
                            a(href="../navbar-fixed-top/") Fixed top
        div.container
            block content
        script(src='/js/jquery.min.js')
        script(src='/js/bootstrap.min.js')
        script(src='/js/angular.min.js')
        script(src='/js/socket.io.js')
        script(src='/js/knockout-2.3.0.js')
        script(src='/js/knockout.mapping-latest.js')
        block scripts